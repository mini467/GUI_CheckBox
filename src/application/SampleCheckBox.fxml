<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CheckBoxController">
   <children>
      <HBox prefHeight="300.0" prefWidth="800.0">
         <children>
            <VBox prefHeight="300.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="CheckBox Объект">
                     <font>
                        <Font name="System Bold Italic" size="18.0" />
                     </font>
                  </Label>
                  <CheckBox fx:id="checkBoxPineapple" mnemonicParsing="false" text="Ананас" />
                  <CheckBox fx:id="checkBoxPepperoni" mnemonicParsing="false" text="Пепперони" />
                  <CheckBox fx:id="checkBoxBacon" mnemonicParsing="false" text="Бекон" />
                  <Button mnemonicParsing="false" onAction="#labelCheckBoxButtonPush" text="Заказать">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Button>
                  <Label fx:id="labelCheckBox" alignment="TOP_LEFT" prefHeight="136.0" prefWidth="201.0" text="Label" />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
